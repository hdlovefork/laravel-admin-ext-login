    "type": "library",
    "keywords": ["laravel-admin", "extension","login"],
    "homepage": "https://github.com/hdlovefork/laravel-admin-ext-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Dean",
            "email": "1602264241@qq.com"
        }
    ],
    "require": {
